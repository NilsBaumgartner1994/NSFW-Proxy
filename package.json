{
  "name": "nsfw-proxy",
  "version": "1.0.0",
  "description": "NodeJS Sequelize FrameWork Proxy",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "rm -rf ./dist/ && webpack --mode development",
    "doc": "./node_modules/.bin/apidoc -i src/ -i models/ -i doc/customDocumentation/ -o doc/",
    "upload": "npm run build && npm version patch && npm publish"
  },
  "apidoc": {
    "template": {
      "forceLanguage": "en"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NilsBaumgartner1994/NSFW-Proxy.git"
  },
  "keywords": [
    "sequelize",
    "proxy",
    "framwork"
  ],
  "bugs": {
    "url": "https://github.com/NilsBaumgartner1994/NSFW-Proxy/issues"
  },
  "homepage": "https://github.com/NilsBaumgartner1994/NSFW-Proxy#readme",
  "author": "Nils Baumgartner",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "1.18.3",
    "cors": "^2.8.5",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "http2": "^3.3.7",
    "multer": "^1.4.1",
    "path": "^0.12.7",
    "util": "^0.12.0",
    "webpack": "^4.35.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^3.10.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.0.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  }
}
